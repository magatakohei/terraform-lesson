[
  {
    "name": "alpine",
    "image": "alpine:3.17.1",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "batch",
        "awslogs-group": "/dev/ecs-scheduled-tasks/example"
      }
    },
    "command": ["/bin/date"]
  }
]
